    "coverage": "babel-node ./node_modules/istanbul/lib/cli cover _mocha",
    "release": "standard-version",
    "prepublish": "npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/peerigon/extract-loader.git"
  },
  "keywords": [
